{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FashionStore": {
    "EndPoint": "https://localhost:44342/api/",
    "Categories": {
      "GetCategory": "category?MaxResultCount=10",
      "CreateCategory": "category",
      "DeleteCategory": "category",
